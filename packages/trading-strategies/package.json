{
  "bugs": {
    "url": "https://github.com/bennycode/trading-signals/issues"
  },
  "dependencies": {
    "@typedtrader/exchange": "0.2.5",
    "@types/big.js": "^6.2.2",
    "big.js": "^7.0.1",
    "trading-signals": "7.4.3"
  },
  "description": "Trading strategies to run trading bots with JavaScript / TypeScript.",
  "devDependencies": {
    "tsx": "^4.21.0"
  },
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/start",
    "!dist/test",
    "!dist/**/*.test.*"
  ],
  "homepage": "https://bennycode.com/trading-signals",
  "keywords": [
    "trading"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "name": "trading-strategies",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf --glob src/**/*{.js,.js.map,.d.ts}",
    "dist": "npm run clean && npm run build",
    "start": "npm run test:types && tsx watch src/start.ts",
    "test": "npm run test:types && npm run test:units",
    "test:types": "tsc --noEmit",
    "test:units": "vitest run --passWithNoTests",
    "test:units:watch": "vitest --watch"
  },
  "type": "module",
  "version": "0.3.5"
}
