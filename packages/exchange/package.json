{
  "bugs": {
    "url": "https://github.com/bennycode/trading-signals/issues"
  },
  "dependencies": {
    "@types/big.js": "^6.2.2",
    "@types/jsonabc": "^2.3.3",
    "axios": "^1.13.4",
    "axios-retry": "^4.5.0",
    "big.js": "^7.0.1",
    "dotenv-defaults": "^5.0.2",
    "jsonabc": "^2.3.1",
    "ms": "4.0.0-nightly.202508271359",
    "ts-retry-promise": "^0.8.1",
    "zod": "^4.3.6"
  },
  "description": "Trading strategies to run trading bots with JavaScript / TypeScript.",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/start",
    "!dist/test",
    "!dist/**/*.test.*"
  ],
  "homepage": "https://bennycode.com/trading-signals",
  "keywords": [
    "trading",
    "typedtrader"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "name": "@typedtrader/exchange",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --dts --format esm --format cjs",
    "clean": "rimraf --glob src/**/*{.js,.js.map,.d.ts}",
    "dist": "npm run clean && npm run build && npm run test:exports",
    "lint:types": "tsc --noEmit",
    "start": "npm run test:types && tsx watch src/start.ts",
    "test": "npm run test:types && npm run test:imports && npm run test:units -- --coverage",
    "test:exports": "attw --pack . --profile node16",
    "test:imports": "depcruise -c ../../.dependency-cruiser.cjs src --include-only \"^src\"",
    "test:types": "npm run lint:types",
    "test:units": "vitest run --passWithNoTests",
    "test:units:watch": "vitest --watch"
  },
  "type": "module",
  "version": "0.2.5"
}
